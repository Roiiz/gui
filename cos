Key:NewKeybind("Escolher Botão UI", "", Enum.KeyCode.Tab, function()
	Library:ToggleUI()
end)

Key:NewKeybind("Menu Keybind", "", Enum.KeyCode.RightShift, function()
    local args = {
        [1] = "Grab",
        [2] = {}}
     game:GetService("Players").LocalPlayer.RemoteEvent:FireServer(unpack(args))
     
     wait(0.5)
     
     local CFrame = CFrame.new(120091, 213.45082092285, 898.25079345703)
     
     local motion
     
     for i, v in pairs(getloadedmodules()) do
        if v.Name == "MotionTracker" then
            motion = v
        end
     end
     
     for i2, v2 in pairs(debug.getupvalues(require(motion))) do
        if type(v2) == "function" then
            if getinfo(v2).name == "TeleportTo" then
                setupvalue(v2, 3, CFrame)
            end
        end
     end
end)






function shelter()
    spawn(function()
            while wait() do
                if not getgenv().autoShelter then break end
                    game:GetService("Players").LocalPlayer.RemoteEvent:FireServer(UpdateShelter, Total)      
            end
    end)
end

function drink()
    spawn(function()
            while wait(20) do
                if not getgenv().autoDrink then break end
                game:GetService("Players").LocalPlayer.RemoteEvent:FireServer(unpack(args))
                        
            end
    end)
end

function stamina()
    spawn(function()
            while wait() do
                if not getgenv().autoStamina then break end
                for i,v in pairs(game:GetService("Players").LocalPlayer:GetChildren()) do
                    if v.Name == "Stamina" then
                        if v.Value <= 45 then
                    v.Value = 100
                        end
                 end
                 end
            end
    end)
end

function pack()
    spawn(function()
            while wait(1) do
                if not getgenv().autoPack then break end
                    local args = {
                        [1] = "Join",
                        [2] = "Parado_xo"}
                    game:GetService("Players").LocalPlayer.RemoteEvent:FireServer(unpack(args))
                    local args = {
                        [1] = "Join",
                        [2] = "Unkso"}
                    game:GetService("Players").LocalPlayer.RemoteEvent:FireServer(unpack(args))
            end
    end)
end

function grab()
    spawn(function()
                while wait(0.5) do
                if not getgenv().antiGrab then break end
                    local args = {
                        [1] = "Grab",
                        [2] = {}}
                    game:GetService("Players").LocalPlayer.RemoteEvent:FireServer(unpack(args))
                end
    end)
end
