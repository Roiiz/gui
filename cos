local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Roiiz/gui/main/da"))()

local CFrame = CFrame.new(120091, 213.45082092285, 898.25079345703)
local motion
local getupvs = debug.getupvalues or getupvalues or getupvals
local getlm = getloadedmodules or get_loaded_modules

local UpdateShelter = "UpdateShelter"
local Total = "Total"
local args = {
    [1] = "drink",
    [2] = workspace.Water.Model}

local Window = Library.CreateLib("", "DarkTheme")
local Main = Window:NewTab("Player")
local Player = Main:NewSection("Dinosaur")

local P = Player:NewButton("Instant Menu", "", function()
    for i, v in pairs(getlm()) do
        if v.Name == "MotionTracker" then
            motion = v
        end
        end
        for i2, v2 in pairs(getupvs(require(motion))) do
        if type(v2) == "function" then
            if getinfo(v2).name == "TeleportTo" then
                setupvalue(v2, 3, CFrame)
            end
        end
    end
end)

local P = Player:NewToggle("Auto Drink", "", function(state)
    getgenv().autoDrink = state
                if state then
                drink()
    end
end)

local P = Player:NewToggle("Auto Shelter", "", function(state)
    getgenv().autoShelter = state
                if state then
                shelter()
    end
end)



Player:NewLabel("Misc")

getgenv().autoShelter = true
getgenv().autoDrink = true


local P = Player:NewButton("Load Players", "", function()

    local infRendDistance = Instance.new("BoolValue")
    infRendDistance.Name = "IgnoreDistanceLimits"
    infRendDistance.Value = true
    infRendDistance.Parent = game:GetService("Players").LocalPlayer.Character

end)
    
local P = Player:NewButton("AFK Mushroom Farm (DO NOT PRESS!!!)>>>>", "1st Slot Empty + Kiiwin Needed", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/Roiiz/gui/main/farm'))()

end)

Player:NewLabel("Others")

local P = Player:NewButton("V.G Hub", "Infinite Stamina + Auto Food", function()
            loadstring(game:HttpGet('https://raw.githubusercontent.com/1201for/V.G-Hub/main/V.Ghub'))()

end)




function shelter()
    spawn(function()
            while wait(0) do
                if not getgenv().autoShelter then break end
                    game:GetService("Players").LocalPlayer.RemoteEvent:FireServer(UpdateShelter, Total)      
            end
    end)
end

function drink()
    spawn(function()
            while wait() do
                if not getgenv().autoDrink then break end
                game:GetService("Players").LocalPlayer.RemoteEvent:FireServer(unpack(args))
                        
            end
    end)
end

Player:NewKeybind("Tab to Hide UI", "", Enum.KeyCode.Tab, function()
	Library:ToggleUI()
end)

