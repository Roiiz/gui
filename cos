local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Roiiz/gui/main/da"))()

local UpdateShelter = "UpdateShelter"
local Total = "Total"
local args = {
    [1] = "drink",
    [2] = workspace.Water.Model}

local Window = Library.CreateLib("", "DarkTheme")
local Main = Window:NewTab("Sonaria")
local Player = Main:NewSection("Player")

local P = Player:NewButton("Menu", "", function()
    local args = {
        [1] = "Grab",
        [2] = {}
     }
     
     game:GetService("Players").LocalPlayer.RemoteEvent:FireServer(unpack(args))
     
     wait(0.5)
     
     local CFrame = CFrame.new(120091, 213.45082092285, 898.25079345703)
     
     local motion
     
     for i, v in pairs(getloadedmodules()) do
        if v.Name == "MotionTracker" then
            motion = v
        end
     end
     
     for i2, v2 in pairs(debug.getupvalues(require(motion))) do
        if type(v2) == "function" then
            if getinfo(v2).name == "TeleportTo" then
                setupvalue(v2, 3, CFrame)
            end
        end
     end
end)

local P = Player:NewButton("Mostrar Vida", "", function()
            local CharacterCtr = workspace.Dinosaurs
            function MakeHPVis(character)
            local head = character:WaitForChild("Head")
            local hp = head:WaitForChild("HealthGui")
            hp.Enabled = true
end
CharacterCtr.ChildAdded:Connect(MakeHPVis)
for _, character in pairs(CharacterCtr:GetChildren()) do
  MakeHPVis(character)
end
end)

local P = Player:NewToggle("Água", "", function(state)
    getgenv().autoDrink = state
                if state then
                drink()
    end
end)

local P = Player:NewToggle("Stamina", "", function(state)
    getgenv().autoStamina = state
                if state then
                stamina()
    end
end)

local P = Player:NewToggle("Anti Clima", "", function(state)
    getgenv().autoShelter = state
                if state then
                shelter()
    end
end)



Player:NewLabel("Funções")

getgenv().autoShelter = true
getgenv().autoDrink = true
getgenv().autoStamina = true


local P = Player:NewButton("Criar Pack", "", function()
            local ohString1 = "CreatePack"
            local ohString2 = "Ander"
            game:GetService("Players").LocalPlayer.RemoteEvent:FireServer(ohString1, ohString2)
            wait(1)
            local ohString1 = "Invite"
            local ohInstance2 = game:GetService("Players").Unkso
            game:GetService("Players").LocalPlayer.RemoteEvent:FireServer(ohString1, ohInstance2)
            local ohString2 = "Invite"
            local ohInstance3 = game:GetService("Players").Parado_xo
            game:GetService("Players").LocalPlayer.RemoteEvent:FireServer(ohString2, ohInstance3)
end)

local P = Player:NewButton("Carregar Players", "", function()

    local infRendDistance = Instance.new("BoolValue")
    infRendDistance.Name = "IgnoreDistanceLimits"
    infRendDistance.Value = true
    infRendDistance.Parent = game:GetService("Players").LocalPlayer.Character

end)
    


Player:NewLabel("Outros")


local P = Player:NewButton("ESP Customizavel", "", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/ic3w0lf22/Unnamed-ESP/master/UnnamedESP.lua',true))()

end)

local P = Player:NewButton("Criar ESP", "", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/Roiiz/gui/main/a',true))()

end)

local P = Player:NewButton("Mushroom Farm", "1 Slot Vazio + Kiwiin", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/Roiiz/gui/main/farm'))()

end)

local P = Player:NewKeybind("Escolher Botão UI", "", Enum.KeyCode.Tab, function()
	Library:ToggleUI()
end)




function shelter()
    spawn(function()
            while wait() do
                if not getgenv().autoShelter then break end
                    game:GetService("Players").LocalPlayer.RemoteEvent:FireServer(UpdateShelter, Total)      
            end
    end)
end

function drink()
    spawn(function()
            while wait() do
                if not getgenv().autoDrink then break end
                game:GetService("Players").LocalPlayer.RemoteEvent:FireServer(unpack(args))
                        
            end
    end)
end

function stamina()
    spawn(function()
            while wait() do
                if not getgenv().autoStamina then break end
                for i,v in pairs(game:GetService("Players").LocalPlayer:GetChildren()) do
                    if v.Name == "Stamina" then
                    v.Value = 100
                 end
                 end
            end
    end)
end

