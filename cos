local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Roiiz/gui/main/ui"))()

local UpdateShelter = "UpdateShelter"
local Total = "Total"
local args = {
    [1] = "drink",
    [2] = workspace.Water.Model}

local Window = Library.CreateLib("Creatures Of Sonaria Script Feito Pra Giulia", "DarkTheme")
local Principal = Window:NewTab("Principal")
local Player = Principal:NewSection("Player")
local Mapa = Window:NewTab("Mapa")
local Mapa = Mapa:NewSection("Planícies")
local Farm = Window:NewTab("Farm/Missões")
local Mush = Farm:NewSection("Mushroom")
local Config = Window:NewTab("Configurar")
local Key = Config:NewSection("Keybinds (Clica pra mudar)")

getgenv().autoShelter = true
getgenv().autoDrink = true
getgenv().autoStamina = true
getgenv().autoPack = true
getgenv().antiGrab = true


Player:NewButton("Menu", "", function()
    local args = {
        [1] = "Grab",
        [2] = {}}
     game:GetService("Players").LocalPlayer.RemoteEvent:FireServer(unpack(args))
     
     wait(0.5)
     
     local CFrame = CFrame.new(120091, 213.45082092285, 898.25079345703)
     
     local motion
     
     for i, v in pairs(getloadedmodules()) do
        if v.Name == "MotionTracker" then
            motion = v
        end
     end
     
     for i2, v2 in pairs(debug.getupvalues(require(motion))) do
        if type(v2) == "function" then
            if getinfo(v2).name == "TeleportTo" then
                setupvalue(v2, 3, CFrame)
            end
        end
     end
end)

Player:NewButton("Mostrar Vida", "", function()
            local CharacterCtr = workspace.Dinosaurs
            function MakeHPVis(character)
            local head = character:WaitForChild("Head")
            local hp = head:WaitForChild("HealthGui")
            hp.Enabled = true
end
CharacterCtr.ChildAdded:Connect(MakeHPVis)
for _, character in pairs(CharacterCtr:GetChildren()) do
  MakeHPVis(character)
end
end)

Player:NewToggle("Água", "", function(state)
    getgenv().autoDrink = state
                if state then
                drink()
    end
end)

Player:NewToggle("Stamina", "", function(state)
    getgenv().autoStamina = state
                if state then
                stamina()
    end
end)

Player:NewToggle("Anti Clima", "", function(state)
    getgenv().autoShelter = state
                if state then
                shelter()
    end
end)

Player:NewToggle("Anti Grab", "", function(state)
    getgenv().antiGrab = state
                if state then
                grab()
    end
end)

Player:NewButton("Criar Pack", "", function()

            game:GetService("Players").LocalPlayer.RemoteEvent:FireServer(unpack(args))
            local ohString1 = "CreatePack"
            local ohString2 = "Ander"
            game:GetService("Players").LocalPlayer.RemoteEvent:FireServer(ohString1, ohString2)
            wait(1)
            local ohString1 = "Invite"
            local ohInstance2 = game:GetService("Players").Unkso
            game:GetService("Players").LocalPlayer.RemoteEvent:FireServer(ohString1, ohInstance2)
            local ohString2 = "Invite"
            local ohInstance3 = game:GetService("Players").Parado_xo
            game:GetService("Players").LocalPlayer.RemoteEvent:FireServer(ohString2, ohInstance3)
end)

Player:NewButton("Carregar Players", "Carrega os Players do Server", function()

    local infRendDistance = Instance.new("BoolValue")
    infRendDistance.Name = "IgnoreDistanceLimits"
    infRendDistance.Value = true
    infRendDistance.Parent = game:GetService("Players").LocalPlayer.Character

end)
    


Player:NewLabel("Outros")

Player:NewToggle("Ativar Tudo", "Ativa Todos os Toggles", function(state)
    getgenv().autoShelter = state
    getgenv().autoDrink = state
    getgenv().autoStamina = state
    getgenv().antiGrab = state
                if state then
                shelter()
                stamina()
                drink()              
                grab()
    end
end)

Player:NewButton("ESP Customizavel", "", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/ic3w0lf22/Unnamed-ESP/master/UnnamedESP.lua',true))()

end)

Player:NewButton("Aimbot", "", function()
    -- Please consider using the newer versions of CheatX, this is broken and buggy :(
if syn then else
    loadstring(game:HttpGet('https://raw.githubusercontent.com/2dgeneralspam1/lua-releases/main/iris-compat.lua'))()
    end
    loadstring(game:HttpGet('https://grfbin.ml/cheatx/old-script.lua'))()

end)

Mapa:NewButton("Planície 1 ", "", function()
    local CFrame = CFrame.new(-37, 208, 966)
    local motion
    local getupvs = debug.getupvalues or getupvalues or getupvals
    local getlm = getloadedmodules or get_loaded_modules
    
    for i, v in pairs(getlm()) do
       if v.Name == "MotionTracker" then
           motion = v
       end
    end
    
    for i2, v2 in pairs(getupvs(require(motion))) do
       if type(v2) == "function" then
           if getinfo(v2).name == "TeleportTo" then
               setupvalue(v2, 3, CFrame)
           end
       end
    end


end)

